{
  "author": "Dario Sechi",
  "devDependencies": {
    "@aracna/core": "^1.1.41",
    "@aracna/react": "^1.0.26",
    "@aracna/web": "^1.0.35",
    "@aracna/web-components": "^1.0.34",
    "@floating-ui/dom": "^1.5.1",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "dompurify": "^3.0.5",
    "esbuild": "^0.19.2",
    "esbuild-plugin-globals": "^0.2.0",
    "focus-trap": "^7.5.2",
    "glob": "^10.3.3",
    "jsdom": "^22.1.0",
    "lit": "^2.8.0",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tabbable": "^6.2.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./components/aria/*": {
      "import": "./components/aria/*.js",
      "require": "./components/aria/*.cjs",
      "types": "./components/aria/*.d.ts"
    },
    "./components/core/*": {
      "import": "./components/core/*.js",
      "require": "./components/core/*.cjs",
      "types": "./components/core/*.d.ts"
    },
    "./components/data/*": {
      "import": "./components/data/*.js",
      "require": "./components/data/*.cjs",
      "types": "./components/data/*.d.ts"
    },
    "./components/feedback/*": {
      "import": "./components/feedback/*.js",
      "require": "./components/feedback/*.cjs",
      "types": "./components/feedback/*.d.ts"
    },
    "./components/input/*": {
      "import": "./components/input/*.js",
      "require": "./components/input/*.cjs",
      "types": "./components/input/*.d.ts"
    },
    "./components/layout/*": {
      "import": "./components/layout/*.js",
      "require": "./components/layout/*.cjs",
      "types": "./components/layout/*.d.ts"
    },
    "./components/navigation/*": {
      "import": "./components/navigation/*.js",
      "require": "./components/navigation/*.cjs",
      "types": "./components/navigation/*.d.ts"
    },
    "./components/surface/*": {
      "import": "./components/surface/*.js",
      "require": "./components/surface/*.cjs",
      "types": "./components/surface/*.d.ts"
    }
  },
  "license": "MIT",
  "main": "index.cjs",
  "module": "index.js",
  "name": "@aracna/react-components",
  "peerDependencies": {
    "@aracna/core": ">= 1.1.0",
    "@aracna/react": ">= 1.0.0",
    "@aracna/web": ">= 1.0.0",
    "@aracna/web-components": ">= 1.0.0",
    "react": ">= 18.2.0"
  },
  "repository": "https://github.com/aracna/react-components",
  "scripts": {
    "test": "yarn vitest run",
    "build": "rm -rf dist && node esbuild.mjs && yarn tsc",
    "tarball": "yarn version && cp LICENSE package.json README.md dist/. && cd dist && yarn pack",
    "deploy": "yarn test && yarn build && yarn tarball && yarn publish dist/*.tgz"
  },
  "sideEffects": false,
  "type": "module",
  "version": "1.0.32"
}
